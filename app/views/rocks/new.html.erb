<h1>Create a Rock</h1>


<%= form_for @rock, url: {action: "create"}, html: {class: "nifty_form"} do |f| %>
  <div class="field">
    <%= f.label :location_found %><br>
    <%= f.text_field :location_found %>
  </div>
  <div class="field">
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </div>
  <div class="field">
    <%= f.label :image %><br>
    <%= f.file_field :image %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
